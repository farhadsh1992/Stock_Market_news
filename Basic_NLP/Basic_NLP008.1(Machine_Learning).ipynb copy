   "metadata": {},
   "outputs": [],
   "source": [
    "\"\"\" \n",
    "number = []\n",
    "for num,text in enumerate(df_data.text):\n",
    "    words = text.split(' ')\n",
    "    if len(words)<=1:\n",
    "        df_data.drop([num],axis=0)\n",
    "        number.append(num)\n",
    "df_data = df_data.reset_index()\n",
    "\"\"\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 70,
   "metadata": {},
   "outputs": [],
   "source": [
    "\"\"\"\n",
    "%%time\n",
    "for num,text in enumerate(df_data.text):\n",
    "    words = text.split(' ')\n",
    "    row = []\n",
    "    for w in words:\n",
    "        if len(w)>1:\n",
    "            row.append(w)\n",
    "    df_data.text[num] = ' '.join(row)\n",
    "#df_data = df_data.reset_index()\n",
    "\"\"\""
   ]
  },
